{
	"name": "WinWinTravel frontend test task",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"lint:all": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"lint:file": "eslint --report-unused-disable-directives --max-warnings 0",
		"format:all": "prettier --log-level warn --write .",
		"format:file": "prettier --log-level warn --write",
		"format:check:all": "prettier --log-level warn --check .",
		"preview": "vite preview",
		"prepare": "husky install",
		"test": "vitest",
		"coverage": "vitest run --coverage"
	},
	"dependencies": {
		"@tanstack/react-query": "^5.74.3",
		"date-fns": "^2.30.0",
		"i18next": "^23.16.8",
		"immer": "^10.1.1",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-i18next": "^13.5.0",
		"react-router": "^6.30.0",
		"react-router-dom": "^6.30.0",
		"vite-tsconfig-paths": "^4.3.2",
		"zustand": "^5.0.3"
	},
	"devDependencies": {
		"@tailwindcss/vite": "^4.1.3",
		"@tanstack/eslint-plugin-query": "^5.73.3",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^14.3.1",
		"@trivago/prettier-plugin-sort-imports": "^4.3.0",
		"@types/node": "^20.17.30",
		"@types/react": "^19.1.1",
		"@types/react-dom": "^19.1.2",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"@vitejs/plugin-react-swc": "^3.8.1",
		"@vitest/coverage-istanbul": "^1.6.1",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-i18next": "^6.1.1",
		"eslint-plugin-prefer-arrow": "^1.2.3",
		"eslint-plugin-prettier": "^5.2.6",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-react-refresh": "^0.4.19",
		"husky": "^8.0.3",
		"jsdom": "^23.2.0",
		"lint-staged": "^15.5.1",
		"prettier": "^3.5.3",
		"tailwindcss": "^4.1.3",
		"tw-animate-css": "^1.2.5",
		"typescript": "^5.8.3",
		"vite": "^6.2.6",
		"vite-plugin-svgr": "^4.3.0",
		"vitest": "^1.6.1"
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"npm run format:file",
			"npm run lint:file"
		]
	}
}
